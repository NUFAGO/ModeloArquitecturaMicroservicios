    title "Contexto - Inacons ERP (Nivel 1)"
    description "Vista de contexto: el sistema Inacons ERP completo"

    // ===== USUARIOS INTERNOS (ARRIBA) =====
    group "üë• Usuarios Internos" {
      include empleado with {
        size md
        color green
      }
      include admin with {
        size md
        color green
      }
    }

    // ===== SISTEMA PRINCIPAL (CENTRO) =====
    include inacons with {
      size lg
      color primary
    }

    // ===== ACTORES EXTERNOS (ABAJO) =====
    group "üåê Actores Externos" {
      include proveedor with {
        size md
        color blue
      }
      include cliente with {
        size md
        color blue
      }
    }

    // ===== ESTILOS DIFERENCIADORES =====
    style * {
      color secondary
    }

    style empleado, admin {
      color green
      textSize lg
    }

    style proveedor, cliente {
      color blue
      textSize lg
    }

    style inacons {
      color primary
      textSize xl
      size lg
    }
